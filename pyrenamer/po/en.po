#: ../data/pyrenamer.glade.h:24
msgid ""
"Image EXIF data replacements:\n"
"{imagewidth}\t\t\t640\n"
"{imageheight}\t\t\t480\n"
"{cameramaker}\t\t\tNikon\n"
"{cameramodel}\t\t\tD-50\n"
"{imagedate}\t\t\t22feb1980\n"
"{imageyear}\t\t\t1980\n"
